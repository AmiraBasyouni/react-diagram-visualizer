{
  "name": "react-diagram-visualizer",
  "version": "0.1.1",
  "description": "Turn a React codebase into an interactive diagram",
  "type": "module",
  "scripts": {
    "predev": "rimraf .parcel-cache dist",
    "dev": "parcel public/index.html",
    "build": "parcel build public/index.html",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "commit": "./scripts/generate-commit-msg.sh",
    "new-component": "new-component",
    "lint": "npx eslint . --fix",
    "format": "npx prettier . --write"
  },
  "bin": {
    "react-diagram-visualizer": "./src/cli.js"
  },
  "files": [
    "README.md",
    "LICENSE.md",
    "ROADMAP.md",
    "CONTRIBUTING.md",
    "assets",
    "src",
    "public",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AmiraBasyouni/react-diagram-visualizer.git"
  },
  "keywords": [
    "react",
    "diagram",
    "visualizer",
    "react-flow",
    "component-graph",
    "component-diagram",
    "component-visualizer",
    "react-tooling",
    "schema-visualizer",
    "visual-schema"
  ],
  "author": "Amira Basyouni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AmiraBasyouni/react-diagram-visualizer/issues"
  },
  "homepage": "https://github.com/AmiraBasyouni/react-diagram-visualizer#readme",
  "devDependencies": {
    "@babel/preset-react": "^7.27.1",
    "@eslint/css": "^0.9.0",
    "@eslint/js": "^9.30.1",
    "@eslint/json": "^0.12.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "new-component": "^6.0.0",
    "prettier": "3.6.2",
    "rimraf": "^6.0.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "parcel": "^2.15.4",
    "@xyflow/react": "^12.8.1",
    "elkjs": "^0.10.0",
    "react": "^19.1.0",
    "react-diagram-schema": "^0.3.3",
    "react-dom": "^19.1.0"
  }
}
